
page {
  background-color: white;
}

.fraame {
  background-color: white;
  font-family: '黑体';
  padding-top: 40rpx;
}

.head {
  text-align: center;
  margin-bottom: 20rpx;
}

.head>text {
  display: block;
  font-size: 20rpx;
  color: #B3B3B3;
}

.fill_img>image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.fill {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0 11rpx 0;
  border-bottom: 1rpx solid #E5E5E5;
  margin-left: 20rpx;
  position: relative;
  color: #606060;
  margin-bottom: 10rpx;
}

.ipt {
  color: #cccccc;
  font-size: 24rpx;
}

.fill>text {
  flex: 1;
  font-size: 26rpx;
  color: #606060;
}

.fill>input {
  flex: 4;
  font-size: 24rpx;
}

.radio-group {
  flex: 4;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

.radio-group>image {
  width: 60rpx;
  height: 60rpx;
}

.fill>button {
  width: 100%;
  background-color: #41ACFF;
  margin-right: 20rpx;
  color: white;
  font-size: 30rpx;
}

.fill>button::after {
  border: 0rpx;
}

.page_bd {
  flex: 4;
}

.tihuoWay {
  flex: 4;
  display: flex;
  align-items: center;
  padding: 0 20rpx 0 0;
  font-size: 24rpx;
}

.fill_arrows {
  width: 40rpx;
  height: 40rpx;
  margin-left: auto;
}

/*  */

.select {
  width: 100%;
  position: absolute;
  top: 102rpx;
  left: 0rpx;
  z-index: 2;
  font-size: 24rpx;
}

.select_one {
  width: 100%;
  line-height: 88rpx;
  background-color: white;
}

.bu {
  margin-top: 20rpx;
  border: 0;
}

/*  */
.item-title{
  border-bottom: 1rpx solid #cccccc;
}